func load_text_file(path):
	var f = File.new()
	var err = f.open(path, File.READ)
	if err != OK:
		printerr("Could not open file, error code ", err)
		return ""
	textArray = f.get_as_text().split("_end")
	print(textArray[0])
	f.close()
	return textArray
